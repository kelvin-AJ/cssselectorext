{
  "name": "css-selector-generator",
  "displayName": "CSS selector generator",
  "publisher": "OlamilekanAjibola",
  "description": "Generates CSS with all classes and ids of HTML selected",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "css", "CSS", "Selector", "generator", "Generator"
  ],
  "activationEvents": [
    "onCommand:css-selector-generator.initialize",
    "onFileSystem:css",
    "onLanguage:css",
    "onLanguage:js",
    "onLanguage:html",
    "onLanguage:jsp",
    "onLanguage:py",
    "onStartupFinished"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kelvin-AJ/cssselectorext.git"
  },
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "css-selector-generator.generate",
      "title": "CSS Selector Generator"
    },
    {
      "command": "css-selector-generator.initialize",
      "title": "CSS Selector Initialize"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  },
  "icon": "./img/icon.png"
}
